    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ATLAS2002/resonatejs.git"
  },
  "bugs": {
    "url": "https://github.com/ATLAS2002/resonatejs/issues"
  },
  "homepage": "https://github.com/ATLAS2002/resonatejs",
  "files": [
    "/dist",
    "/src"
  ],
  "keywords": [
    "react",
    "nextjs",
    "resonatejs",
    "state management",
    "react components",
    "react hooks",
    "ui components",
    "web development",
    "javascript",
    "typescript",
    "animations",
    "cursor",
    "dom events"
  ],
  "author": "ATLAS2002 <siddharth211102@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@config/eslint": "workspace:*",
    "@config/typescript": "workspace:*",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "esbuild": "^0.21.5",
    "eslint": "^8.53.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
